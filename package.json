{
  "name": "clio-ui-kit",
  "version": "1.0.0",
  "description": "Sketch assets and documentation for the product development team",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "new": "cp -i clio-ui-kit.sketch ~/Desktop/untitled.sketch && open -a Sketch ~/Desktop/untitled.sketch",
    "app-screens": "open -a Sketch clio-app-screens.sketch",
    "docs": "cd docs && open http://localhost:4000 && gitbook serve",
    "update": "git pull",
    "source": "open -a Sketch clio-ui-kit.sketch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gthierman/ui-kit.git"
  },
